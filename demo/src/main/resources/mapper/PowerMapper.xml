<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.PowerMapper">

    <select id="selectAllPowerDataRecords" resultType="com.example.demo.model.PowerData">
        <![CDATA[
        SELECT
            timestamp,
            power_consumption
        FROM
            power_data
        WHERE
            pm_id = #{pmId}
        ORDER BY
            timestamp ASC
        ]]>
    </select>

</mapper>